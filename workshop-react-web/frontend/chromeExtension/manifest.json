{
    "manifest_version": 3,
    "name": "kalikro",
    "version": "1.0",
    "permissions": [
        "tabs"      ],
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["asfdg.js","background.js", "script.js"],
          "run_at": "document_start",
          "all_frames": true
        }],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}



